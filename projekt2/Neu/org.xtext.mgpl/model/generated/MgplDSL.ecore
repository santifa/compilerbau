    <eStructuralFeatures xsi:type="ecore:EReference" name="var2" eType="#//VARI" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="expr" eType="#//VARI" containment="true"/>
  </eClassifiers>
