        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ElseStmt">
    <eStructuralFeatures xsi:type="ecore:EReference" name="stmt" eType="#//StatementBlock"
        containment="true"/>
