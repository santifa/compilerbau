    <eStructuralFeatures xsi:type="ecore:EReference" name="add" upperBound="-1" eType="#//VARI"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mult" upperBound="-1" eType="#//VARI"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="unary" upperBound="-1"
        eType="#//VARI" containment="true"/>
